{
    "name": "Seo Inspector",
    "version": "1.23",
    "manifest_version": 2,
    "description": "Page Information",
    "permissions": ["activeTab", "tabs"],
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html"
    },
    "icons": { "16": "img/icon-16.png",
           "32": "img/icon-32.png",
          "128": "img/icon-128.png" }
}
