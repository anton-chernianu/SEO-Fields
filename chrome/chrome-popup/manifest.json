{
    "manifest_version": 2,
    "name": "SEO",
    "description": "SEO",
    "version": "1.0",
    "browser_action": {      
        "default_icon": "seobookm.png",
        "default_title": "Seo Exp"
    },

	"icons": { "16": "seobookm.png",
               "48": "seobookm.png",
               "128": "seobookm.png" },
  "permissions": [
      "tabs",
      "http://*/*",
      "https://*/*",
      "storage",
      "activeTab"
  ],
  "background" : {
    "scripts": ["background.js"]
  },
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["background.js"]
  }
  ]

}